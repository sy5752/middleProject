<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
 	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
 	"http://ibatis.apache.org/dtd/dtd-map-2.dtd">

<sqlMap namespace="admininfo">

	<select id="getAdminInfo" parameterClass="admininfoVO" resultClass="admininfoVO">
		SELECT *
		FROM admininfo
		WHERE admin_id = #admin_id#
		AND admin_pass = #admin_pass#
	</select>
	


</sqlMap>




 	
 	